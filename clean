#!/bin/bash
echo Cleaning Cache
if [ -d "output_dev" ]; then
    rm -R output_dev
		echo dev directory cleaned
else
	  echo dev directory does not exist!
fi

if [ -d "output_prod" ]; then
	  rm -R output_prod
		echo prod directory cleaned
else
	  echo prod directory does not exist!
fi
echo Cache Cleaned!
